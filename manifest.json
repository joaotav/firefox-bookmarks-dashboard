{
  "manifest_version": 2,
  "name": "Bookmarks Dashboard",
  "version": "1.0",
  "description": "A modern bookmarks dashboard for new tabs",
  "icons": {
    "48": "icons/icon-48.png",
    "96": "icons/icon-96.png"
  },
  "permissions": [
    "bookmarks",
    "storage"
  ],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "background": {
    "scripts": ["background.js"]
  }
} 